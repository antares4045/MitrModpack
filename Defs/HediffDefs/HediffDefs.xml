<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <HediffDef ParentName="DiseaseBase">
    <defName>antares_Damage_OrangeCapsule</defName>
    <label>божественный хилл</label>
    <description>божественный хилл</description>
    <isBad>false</isBad>
    <initialSeverity>1</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <baseMoodEffect>200</baseMoodEffect>
        <hungerRateFactor>0</hungerRateFactor>
        <InjuryHealingFactor>999999</InjuryHealingFactor>
			  <ImmunityGainSpeed>999999</ImmunityGainSpeed>
        <TrapSpringChance>-1</TrapSpringChance>
			  <BiosculpterOccupantSpeed>999999</BiosculpterOccupantSpeed>
        <IncomingDamageFactor>-1</IncomingDamageFactor>
        <PainShockThreshold>999999</PainShockThreshold>
        <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
        <LifespanFactor>999999</LifespanFactor>
        <RestFallRateFactor>0</RestFallRateFactor>
        <ComfyTemperatureMin>-999999</ComfyTemperatureMin>
        <ComfyTemperatureMax>999999</ComfyTemperatureMax>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Talking</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Eating</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Hearing</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Breathing</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>BloodFiltration</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>BloodPumping</capacity>
            <offset>999</offset>
          </li>
          <li>
            <capacity>Metabolism</capacity>
            <offset>999</offset>
          </li>
        </capMods>
        <hediffGiversCannotGive>
          <li>Carcinoma</li>
          <li>HeartAttack</li>
          <li>BadBack</li>
          <li>Frail</li>
          <li>Cataract</li>
          <li>Dementia</li>
          <li>Alzheimers</li>
          <li>Asthma</li>
          <li>HeartArteryBlockage</li>
          <li>SensoryMechanites</li>
          <li>WoundInfection</li>
          <li>MissingBodyPart</li>
          <li>Misc</li>
          <li>Burn</li>
          <li>ChemicalBurn</li>
          <li>Crush</li>
          <li>Crack</li>
          <li>Cut</li>
          <li>SurgicalCut</li>
          <li>ExecutionCut</li>
          <li>Scratch</li>
          <li>Bite</li>
          <li>Stab</li>
          <li>Gunshot</li>
          <li>Shredded</li>
          <li>Bruise</li>
          <li>Frostbite</li>
          <li>Blindness</li>
          <li>HearingLoss</li>
          <li>TraumaSavant</li>
          <li>ChemicalDamageModerate</li>
          <li>ChemicalDamageSevere</li>
          <li>Cirrhosis</li>
          <li>ResurrectionPsychosis</li>
          <li>Scaria</li>
          <li>Sterilized</li>
          <li>BloodLoss</li>
          <li>DirtInEyes</li>
          <li>SandInEyes</li>
          <li>MudInEyes</li>
          <li>GravelInEyes</li>
          <li>WaterInEyes</li>
          <li>Hypothermia</li>
          <!-- Can't seem to block these, might need more definitions that they are 
            <li>ParalyticAbasia</li>
            <li>Bleeding</li>
          -->
        </hediffGiversCannotGive>
        <makeImmuneTo>
          <li>Flu</li>
          <li>Malaria</li>
          <li>SleepingSickness</li>
          <li>Plague</li>
          <li>WoundInfection</li>
          <li>LungRot</li>
          <li>GutWorms</li>
          <li>MuscleParasites</li>
          <li>FibrousMechanites</li>
          <li>BloodRot</li>
          <li>InfantIllness</li>
        </makeImmuneTo>
      </li>
    </stages>
  </HediffDef>
  <HediffDef ParentName="DiseaseBase">
    <defName>antares_Damage_GreenCapsule</defName>
    <label>божественный ожог</label>
    <description>божественный ожог</description>
    <isBad>false</isBad>
    <initialSeverity>1</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <comps>
      <li Class="HediffCompProperties_Disappears">
        <disappearsAfterTicks>45000~120000</disappearsAfterTicks>
      </li>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-1.5</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
        <label>лёгкий</label>
        <painFactor>0.95</painFactor>
        <vomitMtbDays>4</vomitMtbDays>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.5</setMax>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.2</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.1</offset>
          </li>
        </capMods>
        <mentalStateGivers>
          <li>
            <mentalState>WanderConfused</mentalState>
            <mtbDays>50</mtbDays>
          </li>
        </mentalStateGivers>
      </li>
      <li>
        <minSeverity>0.3</minSeverity>
        <label>средний</label>
        <painFactor>0.5</painFactor>
        <vomitMtbDays>0.25</vomitMtbDays>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.3</setMax>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Talking</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Metabolism</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>-0.3</offset>
          </li>
        </capMods>
        <forgetMemoryThoughtMtbDays>5</forgetMemoryThoughtMtbDays>
        <mentalStateGivers>
          <li>
            <mentalState>WanderConfused</mentalState>
            <mtbDays>5</mtbDays>
          </li>
        </mentalStateGivers>
      </li>
      <li>
        <minSeverity>0.6</minSeverity>
        <label>тяжёлый</label>
        <painFactor>0</painFactor>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.01</setMax>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.98</minSeverity>
        <label>убойный</label>
        <painFactor>0</painFactor>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.1</setMax>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Talking</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Metabolism</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>-0.3</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>


</Defs>